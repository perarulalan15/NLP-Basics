    {
      "cell_type": "code",
      "source": [
        "def text_classification(url):\n",
        "  text = pipe(url)[0]['label']\n",
        "\n",
        "  return text\n",
        "\n",
        "text_classification(input(\"Enter a sentence:\"))"
      ],
